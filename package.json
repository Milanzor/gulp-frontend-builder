{
    "name": "gulp-frontend-builder",
    "version": "1.1.0",
    "description": "Gulp builder for stuff",
    "main": "gulpfile.js",
    "scripts": {
        "gulp": "gulp",
        "compile-all": "gulp fonts:process js:app:process js:vendors:process js:lib:process scss:process",
        "test": "npm run cleanup && npm run test-only && npm run cleanup",
        "test-only": "npm run compile-all -- --config ./test/config.json &&  mocha ./test/tests/output.test.js",
        "travis": "npm run test",
        "cleanup": "rimraf ./test/dist"
    },
    "author": "Milan van As <milanvanas@gmail.com>",
    "license": "MIT",
    "browserslist": [
        "last 2 versions",
        "ie 11"
    ],
    "dependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-babel": "^8.0.0",
        "gulp-bless": "^4.0.1",
        "gulp-chmod": "^3.0.0",
        "gulp-clean": "^0.4.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-ignore": "^3.0.0",
        "gulp-insert": "^0.5.0",
        "gulp-newer": "^1.4.0",
        "gulp-order": "^1.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sane-watch": "^3.0.4",
        "gulp-sass": "^5.1.0",
        "gulp-size": "^4.0.1",
        "gulp-uglify": "^3.0.2",
        "gulp-using": "^0.1.1",
        "gulp-util": "^3.0.8",
        "main-bower-files": "^2.13.3",
        "minimist": "^1.2.6",
        "require-dir": "^1.2.0",
        "sass": "^1.50.0"
    },
    "devDependencies": {
        "chai": "^4.3.6",
        "mocha": "^9.2.2",
        "rimraf": "^3.0.2"
    }
}
